    "@nunocunha/eslint-config": "0.0.1",
    "@types/jest": "29.5.2",
    "@types/uuid": "9.0.1",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "angular-cli-ghpages": "1.0.6",
    "cypress": "12.13.0",
    "eslint": "8.42.0",
    "jest": "29.5.0",
    "jest-fail-on-console": "3.1.1",
    "jest-preset-angular": "13.1.1",
    "npm-run-all2": "6.0.5",
    "ts-node": "~10.9.1",
    "typescript": "4.9.5"
