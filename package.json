  },
  "bugs": {
    "url": "https://github.com/nunocunha/league-of-random/issues"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "npm-run-all -s test:*",
    "test:tsc": "tsc",
    "test:lint": "eslint .",
    "test:lint:fix": "eslint . --fix",
    "test:unit": "jest",
    "test:cypress": "npm-run-all -s test:cypress:*",
    "test:cypress:open": "cypress run --component",
    "test:cypress:component": "cypress run --component",
    "test:cypress:e2e": "cypress run --e2e",
    "deploy": "ng deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.0.4",
